<div class="your-posts-div">
    {{#each posts}}
    <div class="post-card">
        <h3>{{title}}</h3>
        <p>Posted by: {{User.username}} on {{nice_date}}</p>
        <p>{{contents}}</p>
    </div>
    {{/each}}
</div>
<a href="/newPost">
<button id="new-post-btn">New Post</button>
</a>

<script src="/js/dashboard.js"></script>